#%RAML 1.0 DataType
displayName: InvoiceTracking
description: Defines the InvoiceTracking data type

type: object
properties:
  isDelivered: boolean
  deliveredDate: string | nil
  events: 
    type: array
    minItems: 1
    required: true
    items:
      type: object
      properties:
        city: string
        state: string
        description: string
        date: date-only